    public void exchangeRebulidGame(int[][] rebuild,int length,int y){
    	int x=rebuild[8][y];
    	int nexy=0;
    	for(int i=0;i<length;i++){
    		if(i==0){
    			//交换
    			nexy=x;
    			x=rebuild[0][y];
    			rebuild[0][y]=nexy;
    		}else{
    			nexy=rebuild[i][y];
    			rebuild[i][y]=x;
    			x=nexy;
    		}
    	}
    }
    public int getLineOfRow(int[][] gr,int line,int length){
    	boolean ishave=false;
    	int result=-1;
    	for(int i=length-1;i>-1;i--){
    		if(gr[i][line]==0){
    			result= i;
    			ishave=true;
    			break;
    		}
    	}
    	if(!ishave){
    		return -1;//表示该列没有可消除的行
    	}else{
    		return result;
    	}
    }
	public int getIsmovestate() {
		//返回值为0表示消除状态，1，掉落状态，2，正常状态
		 int result =2; 
		for (int f = 0; f < 9; f++) {
			for (int j = 0; j < 9; j++) {
				if (gameRange_cplayer.gameRange[f][j]<0) {
					result = 0;
					break;
				}else if(gameRange_cplayer.gameRange[f][j]==0){
					result=1;
				break;
				}
			}
			if (result==0||result==1) {
				//System.out.println(result);
				break;
			}
		}
	
		//System.out.println(result);
		return result;
	}
	

