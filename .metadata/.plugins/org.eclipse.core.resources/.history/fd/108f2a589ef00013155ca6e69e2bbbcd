package po;

//个人信息对象
public class UserPO {
	private String id;
	private String password;
	private String name;
	private String photo;
	private int maxScore;

	public UserPO() {}
	
	public UserPO(String id,String password,String name,String photo,int maxScore){
		this.id=id;
		this.name=name;
		this.password=password;
		this.photo=photo;
		this.maxScore=maxScore;
	}
	public UserPO(String password,String name,String photo,int maxScore){
		this.name=name;
		this.password=password;
		this.photo=photo;
		this.maxScore=maxScore;
	}
	public String getId() {
		return id;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	public int getMaxScore() {
		return maxScore;
	}
	public void setMaxScore(int maxScore) {
		this.maxScore = maxScore;
	}
	

}
