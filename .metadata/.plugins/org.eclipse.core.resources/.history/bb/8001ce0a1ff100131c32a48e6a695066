package presentation.gameui;

import java.awt.Color;
import java.awt.Font;
import java.awt.Graphics;

import javax.swing.JPanel;



public class singleGameGUI_showpane extends singleGameGUI {
public void initlizeShowpane(){
	ShowTimePanel showpanel1 = new ShowTimePanel();
	showpanel1.setBounds(0, 35, 70, 595);
	ShowTimePanel showpanel2 = new ShowTimePanel();
	showpanel2.setBounds(590, 35, 70, 595);
	pane.add(showpanel1);
	pane.add(showpanel2);
}
class ShowTimePanel extends JPanel {
	private static final long serialVersionUID = 1L;

	@Override
	public void paint(Graphics g) {
		g.setFont(new Font("Î¢ÈíÑÅºÚ", Font.PLAIN, 18));
		g.setColor(new Color(55, 58, 77));
		g.fillRect(0, 0, this.getWidth(), this.getHeight());

	}
}
}
