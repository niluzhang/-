/*每日局数ArrayList<string> daily_game_numbers,string = 每日局数+“#”+日期
每日平均得分 ArrayList<string> daily_average_scores,string = 每日平均得分+“#”+日期
每局得分 ArrayList<string> per_game_scores,string = 每局得分+“#”+日期
最高连击次数 int max_combo_number
总局数 int game_number
平均得分 int average_score
最高分 int max_score
 */
package game;

import java.util.ArrayList;

public class User_info {
	private ArrayList<String> daily_game_numbers;
	private ArrayList<String> daily_average_scores;
	private ArrayList<String> per_game_scores;
	private int max_combo_number;
	private int game_number;
	private int max_score;

	public User_info(ArrayList<String> daily_game_numbers,
			ArrayList<String> daily_average_score,
			ArrayList<String> per_game_scores, int max_combo_number,
			int game_number, int max_score) {
		this.daily_game_numbers = daily_game_numbers;
		this.daily_average_scores = daily_average_score;
		this.per_game_scores = per_game_scores;
		this.max_combo_number = max_combo_number;
		this.game_number = game_number;
		this.max_score = max_score;
	}

	public ArrayList<String> getDaily_game_numbers() {
		return daily_game_numbers;
	}

	public void setDaily_game_numbers(ArrayList<String> daily_game_numbers) {
		this.daily_game_numbers = daily_game_numbers;
	}

	public ArrayList<String> getDaily_average_scores() {
		return daily_average_scores;
	}

	public void setDaily_average_scores(ArrayList<String> daily_average_scores) {
		this.daily_average_scores = daily_average_scores;
	}

	public ArrayList<String> getPer_game_scores() {
		return per_game_scores;
	}

	public void setPer_game_scores(ArrayList<String> per_game_scores) {
		this.per_game_scores = per_game_scores;
	}

	public int getMax_combo_number() {
		return max_combo_number;
	}

	public void setMax_combo_number(int max_combo_number) {
		this.max_combo_number = max_combo_number;
	}

	public int getGame_number() {
		return game_number;
	}

	public void setGame_number(int game_number) {
		this.game_number = game_number;
	}

	public int getMax_score() {
		return max_score;
	}

	public void setMax_score(int max_score) {
		this.max_score = max_score;
	}

}